<AnchorPane prefHeight="426.0" prefWidth="357.0" style="-fx-background-color: skyblue;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainApplication.PatientRegistrationController">
