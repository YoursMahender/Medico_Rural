        <TextField fx:id="ageField" layoutX="150" layoutY="110" />
        <Label layoutX="50" layoutY="150" text="Gender:" />
        <ComboBox fx:id="genderCombo" layoutX="150" layoutY="150" />
        <Label layoutX="50" layoutY="190" text="Phone:" />
        <TextField fx:id="phoneField" layoutX="150" layoutY="190" />
        <Label layoutX="50" layoutY="230" text="Address:" />
        <TextField fx:id="addressField" layoutX="150" layoutY="230" />
        <Button fx:id="registerbutton" layoutX="149.0" layoutY="358.0" onAction="#handleRegister" text="Register" />
      <Label layoutX="48.0" layoutY="271.0" prefHeight="18.0" prefWidth="59.0" text="Username:" />
      <TextField fx:id="usernamefield" layoutX="150.0" layoutY="268.0" />
      <TextField fx:id="passwordfield" layoutX="150.0" layoutY="306.0" />
      <Label layoutX="48.0" layoutY="310.0" prefHeight="18.0" prefWidth="59.0" text="Password:" />
    </children>
</AnchorPane>
